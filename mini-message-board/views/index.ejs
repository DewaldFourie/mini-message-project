<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <% messages.forEach(function(message) { %>
      <div>
        <h3><%= message.text %></h3>
        <h4><%= message.user %></h4>
        <h5><%= message.added %></h5>
      </div>
    <% }); %> 
    <div>
      <a href="/new"><button>Create a new Message</button></a>
    </div>
  </body>
</html>
